    m_speaker_avatar: {fileID: 21300000, guid: 70fb0fc6beb2647a897e9b1683114fce, type: 3}
    m_text: "\u8FD9\u5C31\u662F\u5F53\u524D\u7684\u4EA4\u4E92\u529F\u80FD\u4E86\uFF01\u6B64\u5916\u5C31\u662F\u6309E\u53EF\u4EE5\u89E6\u53D1\u5BF9\u8BDD\uFF0C\u5E76\u4E14\u5728\u5BF9\u8BDD\u7CFB\u7EDF\u8FD0\u4F5C\u7684\u540C\u65F6\uFF0C\u6E38\u620F\u5185\u7684\u89D2\u8272\u5C31\u4E0D\u80FD\u88AB\u73A9\u5BB6\u7684Input\u63A7\u5236\u4E86"
