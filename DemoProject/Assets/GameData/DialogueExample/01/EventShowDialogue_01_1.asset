  m_dialogues:
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: bcb4accdbcb044b98bc839a4ab34d0f6, type: 3}
    m_text: "\u6211\u4EEC\u7684\u5BF9\u8BDD\u7CFB\u7EDF\u4F7F\u7528\u4E86Adobe\u7684\u5F00\u6E90\u5B57\u4F53\u201C\u601D\u6E90\u9ED1\u4F53\u201D\uFF0C\u9ED8\u8BA4\u4F7F\u7528\u7684\u662F36\u5B57\u53F7\u300115\xB0\u659C\u4F53"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u5DE6\u4FA7\u548C\u4E0A\u65B9\u53EF\u4EE5\u663E\u793A\u8BF4\u8BDD\u4EBA\u7684\u540D\u5B57\u548C\u5934\u50CF\u3002\u4F60\u5E94\u8BE5\u5DF2\u7ECF\u770B\u5230\u6211\u4E86~"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 70fb0fc6beb2647a897e9b1683114fce, type: 3}
    m_text: "\u6BCF\u53E5\u8BDD\u7ED3\u675F\u540E\uFF0C\u53F3\u4E0B\u89D2\u4F1A\u51FA\u73B0\u4E00\u4E2A\u5C0F\u52A8\u753B\uFF0C\u8FD9\u65F6\u4F60\u5C31\u53EF\u4EE5\u6309\u786E\u8BA4\u952E(\u7A7A\u683C/\u56DE\u8F66\uFF0C\u624B\u67C4A\u952E\uFF09\u67E5\u770B\u4E0B\u4E00\u6761\u5BF9\u8BDD\u4E86"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u9664\u4E86\u8FD9\u4E9B\u57FA\u64CD\uFF0C\u5BF9\u8BDD\u7CFB\u7EDF\u8FD8\u652F\u6301\u4E86\u4E00\u4E9B\u5BF9\u53D9\u4E8B\u975E\u5E38\u6709\u5E2E\u52A9\u7684\u529F\u80FD\uFF01"
    m_display_method: 3
    m_can_skip: 1
