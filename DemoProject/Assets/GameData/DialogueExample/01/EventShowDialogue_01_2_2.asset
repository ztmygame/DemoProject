  m_Script: {fileID: 11500000, guid: fe4bbcb1463d4d24fa6aa7c77090626f, type: 3}
  m_Name: EventShowDialogue_01_2_2
  m_EditorClassIdentifier: 
  m_state: 1
  m_dialogues:
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 70fb0fc6beb2647a897e9b1683114fce, type: 3}
    m_text: "\u5BF9\u8BDD\u7CFB\u7EDF\u9002\u914D\u4E86\u5BCC\u6587\u672C\uFF08rich
      text\u6216styled text\uFF09\uFF0C\u80FD\u591F\u5728\u4E0D\u5F71\u54CDtyping\u6548\u679C\u7684\u540C\u65F6\u663E\u793A\u5BCC\u6587\u672C\u5904\u7406\u540E\u7684\u6587\u5B57"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u5728\u914D\u7F6E\u65F6\uFF0C\u4F60\u53EF\u4EE5\u901A\u8FC7\u6DFB\u52A0\u6807\u7B7E\uFF0C\u8BA9\u6587\u5B57<size=70>\u53D8\u5927</size>\u3001<size=30>\u53D8\u5C0F</size>\u3001<b>\u52A0\u7C97</b>\uFF0C\u4E5F\u53EF\u4EE5\u6539\u53D8<style=red>\u6587\u672C</style>\u7684<style=blue>\u989C\u8272</style>"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u60F3\u8981\u4E86\u89E3Unity\u63D0\u4F9B\u7684\u6240\u6709\u57FA\u672C\u6807\u7B7E\uFF0C\u8BF7\u53BBUnity\u5B98\u65B9\u6587\u6863\u641C\u7D22\u201C\u5BCC\u6587\u672C\u201D"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u9664\u4E86\u8FD9\u4E9B\u57FA\u672C\u6807\u7B7E\uFF0C\u6211\u4EEC\u7684\u5BF9\u8BDD\u7CFB\u7EDF\u989D\u5916\u5B9A\u4E49\u4E86\u51E0\u4E2A\u7279\u6B8A\u6807\u7B7E\u3002"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u9996\u5148\u662F\u7B49\u5F85\u65F6\u95F4\u6807\u7B7E\uFF0C\u53EF\u4EE5\u5728\u53E5\u5B50\u4E2D\u7684\u4EFB\u610F\u4F4D\u7F6E\u7B49\u5F85\u4EFB\u610F\u65F6\u957F"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u6BD4\u5982\uFF1A\u5728\u8FD9\u91CC<2>\u7B49\u5F85\u4E86\u4E24\u79D2\u3002<0.5>\u8FD9<0.5>\u53E5<0.5>\u8BDD<0.5>\u7684<0.5>\u6BCF<0.5>\u4E00<0.5>\u4E2A<0.5>\u5B57<0.5>\u90FD<0.5>\u4F1A<0.5>\u7B49<0.5>\u5F85<0.5>\u534A<0.5>\u79D2<0.5>\u518D<0.5>\u51FA<0.5>\u73B0"
    m_display_method: 3
    m_can_skip: 0
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 70fb0fc6beb2647a897e9b1683114fce, type: 3}
    m_text: "\u5BF9\u8BDD\u7CFB\u7EDF\u8FD8\u6DFB\u52A0\u4E86<r=\u6CE8\u97F3>ruby
      text</r>\u6807\u7B7E"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 70fb0fc6beb2647a897e9b1683114fce, type: 3}
    m_text: "ruby text\u4F1A\u4FDD\u7559\u6587\u5B57\u7684<r=\u989C\u8272\u7B49><i><style=red>\u57FA\u672C\u5C5E\u6027</style></i></r>\uFF0C\u5E76\u4E14\u4F1A\u8DDF\u7740\u76F8\u5E94\u7684\u6587\u5B57\u4E00\u8D77\u51FA\u73B0\u3002\u771F\u662F\u592A\u9177\u4E86\uFF01"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: bcb4accdbcb044b98bc839a4ab34d0f6, type: 3}
    m_text: "\u8FD8\u53EF\u4EE5\u901A\u8FC7\u5BCC\u6587\u672C\u6807\u7B7E\u6DFB\u52A0\u81EA\u5B9A\u4E49\u7684\u56FE\u7247<sprite=3>"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: bcb4accdbcb044b98bc839a4ab34d0f6, type: 3}
    m_text: "\u73B0\u5728\u4F60\u770B\u5230\u7684\u56FE\u7247\u53EF\u80FD\u4F7F\u7528\u8F93\u5165\u6CD5\u4E5F\u80FD\u6253\u51FA\u6765<sprite=8>\uFF0C\u4F46\u5B83\u4EEC\u90FD\u662F\u4ECE\u56FE\u96C6\u91CC\u627E\u51FA\u6765\u663E\u793A\u7684<sprite=6>"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u5E0C\u671B\u4F60\u53EF\u4EE5\u7075\u6D3B\u8FD0\u7528\u8FD9\u4E9B\u6807\u7B7E\uFF0C\u628A\u5BF9\u8BDD\u5199\u5F97\u7ED8\u58F0\u7ED8\u8272\uFF01"
    m_display_method: 3
    m_can_skip: 1
