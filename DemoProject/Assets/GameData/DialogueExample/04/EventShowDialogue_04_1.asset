  m_dialogues:
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: bcb4accdbcb044b98bc839a4ab34d0f6, type: 3}
    m_text: "\u4E3A\u4E86\u8BA9\u5BF9\u8BDD\u7CFB\u7EDF\u66F4\u7075\u6D3B\uFF0C\u73B0\u5728\u663E\u793A\u5BF9\u8BDD\u3001\u663E\u793A\u9009\u9879\u3001\u5173\u95ED\u5BF9\u8BDD\u6846\u7B49\u90FD\u662F\u4F5C\u4E3A\u4E00\u4E2A\u4E2Aevent\uFF0C\u6309\u7167\u914D\u7F6E\u65F6\u8BBE\u7F6E\u7684\u987A\u5E8F\u4F9D\u6B21\u6267\u884C"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: bcb4accdbcb044b98bc839a4ab34d0f6, type: 3}
    m_text: "\u5982\u679C\u6709\u5176\u5B83\u7684gameplay\u7CFB\u7EDF\u60F3\u8981\u5728\u5BF9\u8BDD\u7CFB\u7EDF\u4E2D\u6267\u884C\u67D0\u6BB5\u903B\u8F91\uFF0C\u4E5F\u53EF\u4EE5\u628A\u8FD9\u6BB5\u903B\u8F91\u5C01\u88C5\u4E3A\u4E00\u4E2Aevent\uFF0C\u914D\u7F6E\u5230\u5BF9\u8BDD\u7CFB\u7EDF\u6267\u884C\u7684event
      sequence\u4E2D"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u8BF4\u8D77\u6765\u6BD4\u8F83\u62BD\u8C61\uFF0C\u603B\u4E4B\u53EF\u4EE5\u7406\u89E3\u4E3A\u5728\u5BF9\u8BDD\u7CFB\u7EDF\u4E2D\uFF0C\u4E5F\u53EF\u4EE5\u5F88\u7075\u6D3B\u5730\u914D\u7F6E\u4E00\u4E9B\u6E38\u620F\u903B\u8F91\uFF0C\u6BD4\u5982\u64AD\u653E\u52A8\u753B\u3001\u589E\u52A0\u597D\u611F\u5EA6\u7B49\u7B49"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: bcb4accdbcb044b98bc839a4ab34d0f6, type: 3}
    m_text: "\u4E3E\u4E2A\u4F8B\u5B50\uFF0C\u8FD9\u53E5\u8BDD\u7ED3\u675F\u540E\uFF0C\u4F1A\u64AD\u653E\u4E00\u4E2A\u79FB\u52A8\u7D2B\u8272\u70B9\u7684\u4F4D\u7F6E\u7684\u52A8\u753B"
    m_display_method: 3
    m_can_skip: 1
