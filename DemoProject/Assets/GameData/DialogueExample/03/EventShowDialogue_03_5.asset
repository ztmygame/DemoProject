  m_dialogues:
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: bcb4accdbcb044b98bc839a4ab34d0f6, type: 3}
    m_text: "\u8FD8\u6709\u4E00\u4E2A\u6BD4\u8F83\u91CD\u8981\u7684\u529F\u80FD\uFF0C\u90A3\u5C31\u662F\u53EF\u4EE5\u914D\u7F6E\u9009\u9879\u5206\u652F\u662F\u5426\u5168\u90E8\u90FD\u88AB\u904D\u5386\u4E00\u904D\u624D\u4F1A\u7ED3\u675F"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: bcb4accdbcb044b98bc839a4ab34d0f6, type: 3}
    m_text: "\u521A\u521A\u7684\u51E0\u6B21\u9009\u62E9\u5176\u5B9E\u90FD\u4E0D\u9700\u8981\u5168\u90E8\u904D\u5386\uFF0C\u9009\u62E9\u5176\u4E2D\u4E00\u4E2A\u5C31\u53EF\u4EE5\u8FDB\u5165\u4E0B\u4E00\u73AF\u8282~"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u4F46\u5728\u201C\u663E\u793A\u6587\u672C\u201D\u90A3\u4E00\u8282\u4E2D\uFF0C\u201C\u5BCC\u6587\u672C\u6807\u7B7E\u201D\u548C\u201C\u6587\u5B57\u663E\u793A\u65B9\u5F0F\u201D\u7684\u9009\u9879\u5176\u5B9E\u90FD\u8981\u88AB\u904D\u5386\u624D\u884C"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 70fb0fc6beb2647a897e9b1683114fce, type: 3}
    m_text: "\u5305\u62EC\u201C\u4E3B\u83DC\u5355\u201D\uFF0C\u4E5F\u5C31\u662F\u9009\u56DB\u4E2A\u529F\u80FD\u6765\u4ECB\u7ECD\u7684\u5206\u652F\uFF0C\u4E5F\u90FD\u8981\u88AB\u904D\u5386\u624D\u884C\u3002\u8BF7\u8010\u5FC3\u5730\u628A\u56DB\u4E2A\u529F\u80FD\u5168\u90E8\u770B\u4E00\u904D\u5427\uFF01"
    m_display_method: 3
    m_can_skip: 1
  - m_speaker_name: miu
    m_speaker_avatar: {fileID: 21300000, guid: 954616e3c701f4e22bd74bbccd87ccc2, type: 3}
    m_text: "\u5230\u6B64\uFF0C\u9009\u9879\u56DE\u590D\u529F\u80FD\u5C31\u4ECB\u7ECD\u5B8C\u4E86"
    m_display_method: 3
    m_can_skip: 1
